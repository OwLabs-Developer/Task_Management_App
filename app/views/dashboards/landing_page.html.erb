<h1> Welcome to OwLabs </h1>

<% if current_user %>
    <div>
        <%= link_to 'Log Out', logout_path, method: :delete %>
    </div>
<% else %>
    <div>
        <%= link_to 'Log In', new_session_path %>
    </div>
    
    <div>
        <%= link_to 'Sign Up', new_user_path %>
    </div>
<% end %>